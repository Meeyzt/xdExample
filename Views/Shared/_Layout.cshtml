<!DOCTYPE html>
<html lang="tr">
<head>
    <title>@ViewData["Title"] - Foodly</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css " rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Great+Vibes&display=swap" rel="stylesheet">

    <link href="~/feliciano-master/css/open-iconic-bootstrap.min.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/animate.css" rel="stylesheet" />

    <link href="~/feliciano-master/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/magnific-popup.css" rel="stylesheet" />

    <link href="~/feliciano-master/css/aos.css" rel="stylesheet" />

    <link href="~/feliciano-master/css/ionicons.min.css" rel="stylesheet" />

    <link href="~/feliciano-master/css/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/jquery.timepicker.css" rel="stylesheet" />

    <link href="~/feliciano-master/css/flaticon.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/icomoon.css" rel="stylesheet" />
    <link href="~/feliciano-master/css/style.css" rel="stylesheet" />



</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="index.html" style="font-size:30px;">Foodly</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item"><a href="/" class="nav-link">Anasayfa</a></li>
                    <li class="nav-item"><a href="/Blogs" class="nav-link">Makaleler</a></li>                    
                    <partial name="_LoginPartial" />
                </ul>
            </div>
        </div>
    </nav>
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    @RenderBody()
    <footer class="ftco-footer ftco-bg-dark ftco-section">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md-6 col-lg-3">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Foodly (Food Likey)</h2>
                        <p>
                            Foodly bir restaurant ve yemek zevkleri tartışma sitesidir.
                            Foodly ismi Yemek beğenilmesi kavramı olan Food Likey den türetilip kısaltılarak "Foodly" olmuştur.
                        </p>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-3">
                            <li class="ftco-animate"><a href="https://github.com/Meeyzt/Foodly_New"><span class="fab fa-instagram"></span></a></li>
                            <li class="ftco-animate"><a href="https://instagram.com/Mehmt.ylmz16"><span class="fab fa-github"></span></a></li>
                            <li class="ftco-animate"><a href="https://www.linkedin.com/in/mehmet-y%C4%B1lmaz-72a95011a/"><span class="fab fa-linkedin-in"></span></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>
                        Copyright &copy;
                        <script>document.write(new Date().getFullYear());</script> All rights reserved | Foodly by <a href="https://github.com/Meeyzt" target="_blank">MeeyzT</a>
                    </p>
                </div>
            </div>
        </div>
    </footer>
    @RenderSection("Scripts", required: false)
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/feliciano-master/js/jquery.min.js"></script>
    <script src="~/feliciano-master/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/feliciano-master/js/popper.min.js"></script>
    <script src="~/feliciano-master/js/bootstrap.min.js"></script>
    <script src="~/feliciano-master/js/jquery.easing.1.3.js"></script>
    <script src="~/feliciano-master/js/jquery.waypoints.min.js"></script>
    <script src="~/feliciano-master/js/jquery.stellar.min.js"></script>
    <script src="~/feliciano-master/js/owl.carousel.min.js"></script>
    <script src="~/feliciano-master/js/jquery.magnific-popup.min.js"></script>
    <script src="~/feliciano-master/js/aos.js"></script>
    <script src="~/feliciano-master/js/jquery.animateNumber.min.js"></script>
    <script src="~/feliciano-master/js/bootstrap-datepicker.js"></script>
    <script src="~/feliciano-master/js/jquery.timepicker.min.js"></script>
    <script src="~/feliciano-master/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/feliciano-master/js/google-map.js"></script>
    <script src="~/feliciano-master/js/main.js"></script>    
</body>
</html>

